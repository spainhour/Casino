<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<VBox style="-fx-background-image: url('feltGreen.jpg');" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HighscoresGUIController">
   <children>
      <Label fx:id="usernameLabel" alignment="CENTER" prefHeight="26.0" prefWidth="600.0" text="username" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel1" alignment="CENTER" prefHeight="26.0" prefWidth="600.0" text="Highscore List" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 15;">
         <top>
            <ChoiceBox fx:id="cb" prefWidth="150.0" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Button fx:id="back" mnemonicParsing="false" onAction="#back" prefWidth="70.0" text="Back" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</VBox>
